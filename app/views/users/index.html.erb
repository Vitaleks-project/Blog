<center><h3>Users list</h3></center>
<div id="users"><% @users.each do |user| %>
  <%= user.id %>
  <%=link_to user.email, user %>
  <%= link_to "Delete", user, :class => 'right', method: :delete,
                                                 data: { confirm: "You sure?" } %>
  <br/>
<% end %>
</div>
